<div class="errorMessage">Please input a name and image for the dog</div>
<form id="newDog" name="newDog" method="post" onsubmit="validateNewDog()" action="/api/createdog" enctype="multipart/form-data">
    <label for="dog_name">Dog Name</label>
    <input name="dog_name" class="dogName" type="text" />
    <label for="inputBreed">Breed</label>
    <select>
        <option value="golden_doodle">Golden Doodle</option>
        <option value="bulldog">Bulldog</option>
    </select>
    <input class="fileUpload" type="file" name="dog_photo">
    Select an Image
    <input class="btn btn-success newDogSubmit" type="submit" value="Submit Dog" />
</form>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/createDog.js"></script>